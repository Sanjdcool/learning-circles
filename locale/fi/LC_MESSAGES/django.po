#, python-format
msgid ""
"Course materials provided by <a href=\"%(link)s\" "
"target=\"_blank\">%(provider)s</a>"
msgstr ""
"Kurssimateriaalit tarjoaa <a href=\"%(link)s\" "
"target=\"_blank\">%(provider)s</a>."

